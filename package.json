{
  "name": "nextjs-template",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "prettier": "prettier --write .",
    "test": "vitest",
    "deps-check": "npm ci && npm-check-updates",
    "deps-upgrade": "npm-check-updates --upgrade --target minor && npm install",
    "vercel-env-pull": "vercel env pull .env.local",
    "vercel-build": "prettier --check . && vitest run && next build"
  },
  "dependencies": {
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "autoprefixer": "10.4.13",
    "eslint": "8.28.0",
    "eslint-config-next": "13.0.5",
    "next": "13.0.5",
    "npm-check-updates": "16.4.3",
    "postcss": "8.4.19",
    "prettier": "2.8.0",
    "prettier-plugin-tailwindcss": "0.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.3",
    "vercel": "28.6.0",
    "vitest": "0.25.3"
  }
}
