{
  "name": "nextjs-template",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18",
    "npm": "9"
  },
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "prettier --check . && vitest run && next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "prettier": "prettier --write .",
    "test": "vitest",
    "deps-check": "npm install && npm-check-updates",
    "deps-upgrade": "npm-check-updates --upgrade --target minor && npm install",
    "env-pull": "vercel env pull .env.local"
  },
  "dependencies": {
    "@types/node": "18.15.7",
    "@types/react": "18.0.29",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "autoprefixer": "10.4.14",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "eslint-plugin-deprecation": "1.4.0-next.1",
    "next": "13.2.4",
    "npm-check-updates": "16.8.0",
    "postcss": "8.4.21",
    "prettier": "2.8.7",
    "prettier-plugin-tailwindcss": "0.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.2.7",
    "typescript": "5.0.2",
    "vercel": "28.18.1",
    "vitest": "0.29.7"
  }
}
