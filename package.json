{
  "name": "nextjs-template",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18",
    "npm": "8"
  },
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "prettier --check . && vitest run && next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "prettier": "prettier --write .",
    "test": "vitest",
    "deps-check": "npm install && npm-check-updates",
    "deps-upgrade": "npm-check-updates --upgrade --target minor && npm install",
    "env-pull": "vercel env pull .env.local"
  },
  "dependencies": {
    "@next/font": "13.2.1",
    "@types/node": "18.14.1",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "autoprefixer": "10.4.13",
    "eslint": "8.34.0",
    "eslint-config-next": "13.2.1",
    "next": "13.2.1",
    "npm-check-updates": "16.7.9",
    "postcss": "8.4.21",
    "prettier": "2.8.4",
    "prettier-plugin-tailwindcss": "0.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.2.7",
    "typescript": "4.9.5",
    "vercel": "28.16.7",
    "vitest": "0.29.1"
  }
}
